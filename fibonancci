package modul8;

/**
 *
 * @author Asisten
 */
public class Modul8 {

    /**
     * @param args the command line arguments
     */
    public static void main(String args[])
	{		
		int prev, next, sum, n;
		prev=next=1;
		for(n=1;n<=10;n++)
		{
			System.out.println(prev);
			sum=prev+next;
			prev=next;
			next=sum;
		}
	}
    
}
